{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/_app.jsx", "utils/Authcontext.js", "pages/api/auth/[...nextauth]/index.js", "pages/api/socket.js", "pages/api/socketio.js", "utils/messageHandler.js", "pages/index.jsx"],
  "exclude": ["node_modules"]
}
